.mui-content{width: 100%;margin-top: 15px;}
.mui-row{padding: 0px 10px;}
.row_a{width: 50%;padding: 20px 10px;box-sizing: border-box;display:inline-block;border-bottom: solid 1px #d0d0d0;}
.row{width: 50%;padding: 20px 10px;box-sizing: border-box;display:inline-block;border-bottom: solid 1px #d0d0d0;border-left: solid 1px #d0d0d0;}
.mui-row image{height: 160px;width: 98%;}
.row-title{overflow: hidden;height: 40px;line-height: 20px;font-size: 14px;margin-bottom: 5px;}
.row-1-3{color: #8e8e8e;font-size: 12px;}
.row-1-3 text{color: red;font-size: 16px;width: 65%; }
.row-1-4{color: #ffffff;background-color: #E94141;width:35%;text-align: center;border-radius:8px;font-size: 14px;height: 25px;line-height: 25px;}





